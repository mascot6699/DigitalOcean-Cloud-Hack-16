{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click here!"
  },
   "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/jquery-3.0.0.js", "js/jquery-ui.min.js", "js/inject.js"],
      "css": ["css/jquery-ui.min.css", "css/mystyle.css"]
    }
  ]
}
