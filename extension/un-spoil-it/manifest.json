{
  "manifest_version": 2,

  "name": "UN SPOIL IT",
  "description": "Removes spoiler so you can read care free",
  "version": "1.0",

  "browser_action": {
    "default_icon": "un-spoil-it.png",
    "default_title": "UN SPOIL IT"
  },
   "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/jquery-3.0.0.js", "js/jquery-ui.min.js", "js/inject.js"],
      "css": ["css/jquery-ui.min.css", "css/mystyle.css"]
    }
  ]
}
